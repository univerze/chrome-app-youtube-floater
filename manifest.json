{
  "name": "Floater for Youtube™",
  "version": "0.0.1",
  "description": "Floating player for Youtube™ videos",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["src/index.js"]
    }
  },
  "icons": {
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": ["browser", "contextMenus", "webview", "alwaysOnTopWindows", "storage", "fullscreen"]
}